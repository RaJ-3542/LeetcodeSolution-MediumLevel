
class Solution {


public:
    void countRes(vector<int>& res, int& cnt){
        for(int i = 0; i <26 ;i++){
            if(res[i]>0){
                res[i]--;
                cnt++;

                countRes(res,cnt);
                res[i]++;
            }
        }
    }

    int numTilePossibilities(string tiles) {
        int intMAx = 256;

        vector<int> res(26,0);


        for(int i = 0; i<tiles.size();i++){
            res[tiles[i]-'A']++;
        }
        int ans = 0;

        countRes(res,ans);

        return ans;
        
    }
};
